#include <iostream>
using namespace std;

class Rectangle {
    int width, height;

public:
    Rectangle(int w, int h) {
        width = w;
        height = h;
    }
    void display() {
        cout << "Width: " << width <<endl;
		cout << "Height: " << height << endl;
    }
};

int main() {
    Rectangle r1(1, 6);
    r1.display(); 

    Rectangle* r2 = &r1;  
    r2->display();
    return 0;
}

